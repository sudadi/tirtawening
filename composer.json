{
  "name": "dotkom/tirtawening",
  "description": "Sipas Tirtawening",
  "license": "MIT",
  "type": "project",
  "homepage": "https://sudadi.my.id",
  "require": {
    "php": "^7.4 || ^8.0",
    "codeigniter4/framework": "^4.0",
    "codeigniter4/shield": "^1.0@beta",
    "longman/telegram-bot": "^0.82.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^9.1"
  },
  "autoload": {
    "exclude-from-classmap": [
      "**/Database/Migrations/**"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Support\\": "tests/_support"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
